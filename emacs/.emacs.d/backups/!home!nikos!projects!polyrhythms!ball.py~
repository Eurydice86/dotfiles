from playsound import playsound

class Ball:
    def __init__(self, _x, _y, _vx, _vy, _note, _speed):
        self.x = _x
        self.y = _y
        self.vx = _vx
        self.vy = _vy
        self.note = _note
        self.speed = _speed

    def print_pos(self):
        print(f"({self.x}, {self.y})")

    def ping(self):
        playsound("./notes/ping-82822.mp3")
        
